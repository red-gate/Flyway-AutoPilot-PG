function "inventory"."updateavailableseats(integer, integer)" {
  text = """

BEGIN
    UPDATE Inventory.Flight
    SET AvailableSeats = AvailableSeats + SeatChange
    WHERE FlightID = FlightID;
END;
"""
  returnType = void
  arguments = <
    {
      name = flightid
      type = integer
      mode = IN
    }

    {
      name = seatchange
      type = integer
      mode = IN
    }

  >
  language = plpgsql
  owner = postgres
}

